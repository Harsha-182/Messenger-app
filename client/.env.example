# Your Auth0 domain (found in your Auth0 dashboard under Applications > Settings)
VITE_DOMAIN=your-auth0-domain.us.auth0.com

# Your Auth0 Client ID (specific to your frontend application)
VITE_CLIENT_ID=your-auth0-client-id

# Your Auth0 API Audience (used to identify the API you're requesting access to)
VITE_AUDIENCE=your-api-audience
